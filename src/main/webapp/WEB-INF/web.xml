<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" version="5.0">
  <display-name>Project</display-name>   
  <servlet>
        <servlet-name>TodoServlet</servlet-name>
        <servlet-class>com.pro.web.TodoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TodoServlet</servlet-name>
        <url-pattern>/post</url-pattern>
    </servlet-mapping>
    
      <servlet>
        <servlet-name>TodoServletGet</servlet-name>
        <servlet-class>com.pro.web.TodoServletGet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TodoServletGet</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>
    
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>